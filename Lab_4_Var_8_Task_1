#include <iostream>
#include <vector>
#include <algorithm>	
using namespace std;

int main()
{
	int n;
	cin >> n;
	cout << "введите количество чисел в векторе" << endl;
	vector <int> vac;

	for (int i = 0; i <= n; i++)
	{
		vac.push_back(i);
	}

	cout << "min = " << vac.front() << endl;
	cout << "max = " << vac.back() << endl;
}
